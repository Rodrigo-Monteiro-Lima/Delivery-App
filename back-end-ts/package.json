{
  "name": "delivery-app-back-end",
  "version": "0.1.0",
  "main": "./build/server",
  "engines": {
    "node": ">=16.0.0"
  },
  "engineStrict": true,
  "private": true,
  "scripts": {
    "build": "/bin/sh tsc_eval.sh",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "start": "node .",
    "dev": "nodemon --watch \"./src/**\" ./src/api/server.ts",
    "dev:ts": "nodemon --watch \"./src/**\" --ext \"ts,js\" --ignore \"src/**/*.spec.ts,src/**/*.json\" --exec \"ts-node src/api/server.ts\"",
    "test": "mocha src/tests/**/*$NAME*.test.js --exit",
    "test:coverage": "nyc --all npm run test",
    "test:coverage:json": "nyc --all --reporter=json-summary npm run test",
    "lint": "eslint --no-inline-config --no-error-on-unmatched-pattern -c .eslintrc.json ."
  },
  "dependencies": {
    "@hapi/boom": "9.1.4",
    "@types/express": "4.17.13",
    "body-parser": "1.19.2",
    "cors": "^2.8.5",
    "dotenv": "16.0.0",
    "express": "4.17.3",
    "express-async-errors": "3.1.1",
    "express-rescue": "1.1.31",
    "jest": "27.4.3",
    "joi": "^17.9.1",
    "jsonwebtoken": "8.5.1",
    "md5": "2.3.0",
    "multer": "^1.4.4",
    "mysql2": "2.3.3",
    "sequelize": "6.17.0",
    "socket.io": "^4.4.1",
    "swagger-ui-express": "^4.6.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@types/bcryptjs": "2.4.2",
    "@types/chai": "4.3.0",
    "@types/chai-http": "4.2.0",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "8.5.6",
    "@types/mocha": "9.1.0",
    "@types/multer": "^1.4.7",
    "@types/node": "16.11.7",
    "@types/sequelize": "4.28.10",
    "@types/sinon": "10.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "13.6.6",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "chai": "4.3.6",
    "chai-http": "4.3.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "15.0.0",
    "eslint-config-trybe-backend": "1.0.4",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-mocha": "9.0.0",
    "eslint-plugin-sonarjs": "0.10.0",
    "mocha": "9.2.2",
    "nodemon": "2.0.15",
    "nyc": "15.1.0",
    "sequelize-cli": "6.4.1",
    "sinon": "13.0.1",
    "ts-node": "10.7.0",
    "ts-node-dev": "1.1.8",
    "typescript": "4.4.4"
  }
}
